<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:com.beta.election.media.client.widgets">
	<ui:style>
		.main{
			width: 100%;
			height: 100%;
		}
		.main .wrapper{
			width: 1200px;
			margin: 70px auto auto auto;
			min-height: 500px;
		}
		.main .wrapper .container{
			max-height: 800px;
		}
	</ui:style>
	<g:HTMLPanel>
		<div class="{style.main}">
			<div class="{style.wrapper}">
				<m:TinyScrollPanel ui:field="container"/>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 