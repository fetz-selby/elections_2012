<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.clearfix{
			height: 0px;
			clear: both;
		}
		.main{
			padding: 0px;
		}
		.main .chart{
			width: 100%;
			height: 300px;
			margin-left: -80px;
		}
		.main .legend{
			width: 100%;
			height: auto;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.main}">
		<g:SimplePanel ui:field="chart" styleName="{style.chart}" />
		<div class="{style.clearfix}"></div>
		<g:SimplePanel ui:field="legend" styleName="{style.legend}"></g:SimplePanel>
	</g:HTMLPanel>
</ui:UiBinder> 