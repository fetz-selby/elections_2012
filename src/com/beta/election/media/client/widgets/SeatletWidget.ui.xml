<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.main{
			float: left;
			width: 115px;
			height: 27px;
			padding: 3px;
			margin-top: 2px;
			margin-bottom: 18px;
			margin-right: 3px;
		}
		.main .logo{
			float: left;
			width: 32px;
			height: 32px;
			margin-right: 3px;
		}
		.main .name{
			float: left;
			padding-left: 2px;
			padding-right: 2px;
			font-size: 23px;
			color: white;
		}
		.main .count{
			float: left;
			font-size: 24px;
			font-weight: bold;
			color: #ffffff;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.main}">
		<g:Image styleName="{style.logo}" ui:field="logo"/>
		<label class="{style.name}" ui:field="partyName"/>
		<label class="{style.name}">-</label>
		<label class="{style.count}" ui:field="voteCount"/>
	</g:HTMLPanel>
</ui:UiBinder> 