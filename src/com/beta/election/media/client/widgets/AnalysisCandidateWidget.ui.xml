<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.main{
	width: 100%;
	max-height: 30px; 
}
.main .wrapper{
	width: 800px;
	height: 100%;
	margin: auto;
}
.main .wrapper .back{
	max-width: 50px;
	max-height: 50px;
	float: left;
}
.main .wrapper .back:hover{
	max-width: 50px;
	max-height: 50px;
	float: left;
	cursor: pointer;
}
.main .wrapper .text_display{
	width: 190px;
	height: auto;
	float: left;
	font-size: 35px;
	margin-left: 5px;
	margin-right: 5px;
	text-align: center;
}
.main .wrapper .forward{
	max-width: 50px;
	max-height: 50px;
	float: left;
}
.main .wrapper .forward:hover{
	max-width: 50px;
	max-height: 50px;
	float: left;
	cursor: pointer;
}
	</ui:style>
	<g:HTMLPanel>
	<div class="{style.main}">
	<div class="{style.wrapper}">
		<g:Image styleName="{style.back}" url="images/br_prev_32.png" ui:field="backButton"/>
		<div class="{style.text_display}" ui:field="canName" />
		<g:Image styleName="{style.forward}" url="images/br_next_32.png" ui:field="forwardButton"/>
	</div>
</div>
	</g:HTMLPanel>
</ui:UiBinder> 